{
  "request": {
    "method": "POST",
    "url": "/v1/parts/delete"
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "responseType": "delete",
      "result": "success",
      "part": {
        "id": "{{{jsonPath request.body '$.part.id'}}}",
        "name": null,
        "description": null,
        "materials": null
      }
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": [
      "response-template"
    ]
  }
}
